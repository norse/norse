{
	"name": "norse",
	"build": {
		"context": "..",
		"dockerfile": "../publish/Dockerfile",
	},
	"customizations": {
		"vscode": {
			"extensions": [
				"ms-python.python",
				"ms-python.vscode-pylance",
				"ms-python.isort",
				"ms-python.debugpy",
				"ms-python.flake8",
				"ms-toolsai.jupyter",
				"ms-python.black-formatter",
				"mutantdino.resourcemonitor"
			],
			"settings": {
                "python.defaultInterpreterPath": "/bin/python3"
            }
		}
	}
}